@model BookTracker.Models.UserShelf

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>UserShelf</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-controller="UserShelves">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">

                <input asp-for="Category" class="form-control" value="2" />

            </div>

            <div class="form-group">

                <input asp-for="Book.Id" class="form-control" type="text" value="1" />

            </div>
            <div class="form-group">

                <input asp-for="UserDetails.EnrollId" class="form-control" type="text" value="3" />

            </div>


            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
